{
  "manifest_version": 2,
  "name": "Trello Shortcuts Killer",
  "version": "1.0.1",
  "description": "Remove shortcuts from Trello for people who don't use them",
  "permissions": [
    "*://trello.com/*"
  ],
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://trello.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "author": "Sosth√®n Gaillard"
}